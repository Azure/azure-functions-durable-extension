{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "DurableTask.AzureStorage": "Warning",
      "DurableTask.Core": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "storageProvider": {
      "type": "mssql",
      "connectionStringName": "SQLDB_Connection",
      "createDatabaseIfNotExists": true
    }
  }
}

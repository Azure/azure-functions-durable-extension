{
  "version": "2.0",
  "functionTimeout": "00:02:00",
  "logging": {
    "logLevel": {
      "DurableTask.AzureStorage": "Information",
      "DurableTask.Core": "Information",
      "Host.Triggers.DurableTask": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false
      }
    }
  },
  "extensions": {
    "durableTask": {
      "hubName": "TimeoutTests",
      "extendedSessionsEnabled": "true",
      "maxEntityOperationBatchSize":  4
    }
  }
}